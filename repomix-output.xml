using FaerieTables.Api.Data;
using FaerieTables.Api.Entities;
using FaerieTables.Api.Services;
using Microsoft.EntityFrameworkCore;

namespace FaerieTables.Api.Tests.Services
{
    public class TableServiceTests
    {
        private TableService CreateService(DbContextOptions<RandomTableContext> options)
        {
            var context = new RandomTableContext(options);
            return new TableService(context);
        }

        private DbContextOptions<RandomTableContext> InMemoryOptions()
        {
            return new DbContextOptionsBuilder<RandomTableContext>()
                .UseInMemoryDatabase(databaseName: $"TestDb_{Guid.NewGuid()}")
                .Options;
        }

        [Fact]
        public async Task CreateAsync_Should_Add_NewTable_To_Db()
        {
            // Arrange
            var options = InMemoryOptions();
            var service = CreateService(options);

            // Act
            var table = new Table { Title = "Test Table" };
            var result = await service.CreateAsync(table);

            // Assert
            Assert.NotEqual(Guid.Empty, result.Id);

            using var ctx = new RandomTableContext(options);
            var count = await ctx.Tables.CountAsync();
            Assert.Equal(1, count);
        }
    }
}
